/* pages/addaddress/index.wxss */
.row-view{
  display: flex;
  flex:1;
  margin-left: 30rpx;
  padding-right: 30rpx;
  height: 104rpx;
  border-bottom: 2rpx solid #e8e8ea;
  align-items: center;
}
.row-title{
  font-size: 16px;
  color: #000000;
  width: 96rpx;
}
.row-content{
  font-size: 16px;
  margin-left: 50rpx;
  display: flex;
  width: 470rpx;
}
.sex-view{
  width: 150rpx;
  height: 60rpx;
  border-radius: 6rpx;
  background-color: #ffda3b;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 30rpx;
}
.sex-text{
  font-size: 14px;
  color:#000000;
}
.sex-view-chooseed{
   width: 150rpx;
  height: 60rpx;
  border-radius: 6rpx;
  border: 1rpx solid #cccccc;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 30rpx;
}
.sex-text-chooseed{
  font-size: 14px;
  color:#666666;
}
.choose-address-icon{
  width:24rpx;
  height:33rpx;
  margin-left: 30rpx;
}
.button-other{
  margin-top: 60rpx;
  width:690rpx;
}